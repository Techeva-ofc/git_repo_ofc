            "className": "org.joget.apps.app.lib.BeanShellTool",
            "properties": {"script": "import java.lang.System;\r\nimport java.lang.String;\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport org.joget.apps.form.model.Element;\r\nimport org.joget.apps.form.model.FormData;\r\nimport org.joget.apps.form.model.FormRow;\r\nimport org.joget.apps.form.model.FormRowSet;\r\nimport org.joget.apps.form.service.FormUtil;\r\nimport org.joget.plugin.base.PluginManager;\r\nimport org.joget.apps.form.model.FormStoreBinder;\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport java.sql.Connection;\r\nimport java.sql.DriverManager;\r\nimport java.sql.ResultSet;\r\nimport java.sql.SQLException;\r\nimport java.sql.PreparedStatement;\r\nimport javax.sql.DataSource;\r\nimport java.sql.*;\r\nimport java.util.Date;\r\nimport java.text.SimpleDateFormat;\r\n\r\n\r\npublic void updatefile()\r\n{\r\n\r\n                PreparedStatement stmt = null;\r\n                PreparedStatement statement = null;\r\n                \r\n\t\t\t\tConnection connection = null;\r\n\r\n        try{\r\n      \r\n\t\t\t\tDataSource ds = (DataSource)AppUtil.getApplicationContext().getBean(\"setupDataSource\");\r\n                 connection = ds.getConnection();\r\n                 \r\n               \r\n\t\t\t\t String files = \"#form.app_fd_ofc_serReqs_tab.docuploadbyclient#\";\r\n\t\t\t\t String id = \"#form.app_fd_ofc_serReqs_tab.id#\";\r\n\t\t\t\t String currentuser = \"#currentUser.fullName#\";\r\n\t\t\t\t \r\n\t\t\t\t SimpleDateFormat dateFormat = new SimpleDateFormat(\"yyyy-MM-dd HH:mm:ss\");\r\n\r\n                // Get current date and time\r\n                Date currentdate = new Date();\r\n                String currentDate = dateFormat.format(currentdate);\r\n\t\t\t\t \r\n\t\t\t\t \r\n\t\t\t\t System.out.println(\"File-----------\"+files);\r\n\t\t\t\t \t System.out.println(\"id-----------\"+id);\r\n\t\t\t\t \r\n\t\t\t\t \r\n\t\t\t\t \r\n\t\t\t\t if(files!=null)\r\n\t\t\t\t {\r\n                //stmt = connection.prepareStatement(\"delete from app_fd_ofc_insp_images where check_point_id =?\");\r\n                //stmt.setString(1,id);\r\n                //stmt.executeQuery();\r\n\t\t\t\t//if(!images.equals(\"\")){\r\n\t\t\t\tString[] file = files.split(\";\");\r\n\t\t\t\t    for(String filename:file)\r\n\t\t\t\t    {\r\n\t\t\t\t    stmt = connection.prepareStatement(\"insert into app_fd_ofc_file_history (file,doctype_id,dateofupload,uploadedby) values (?,?,?,?)\");\r\n\t\t\t\t    stmt.setString(1,filename);\r\n\t\t\t    \tstmt.setString(2,id);\r\n\t\t\t    \tstmt.setString(3,currentDate);\r\n\t\t\t    \tstmt.setString(4,currentuser);\r\n\t\t\t    \tstmt.executeQuery();\r\n\t\t\t    \t}\r\n\t\t\t    \t\r\n\t\t\t    \tstatement = connection.prepareStatement(\"update  app_fd_ofc_serreqs_tab set c_docuploadbyclient=?, c_docuploaded =? where id=? \");\r\n\t\t\t\t    statement.setString(1,null);\r\n\t\t\t    \tstatement.setString(2,files);\r\n\t\t\t    \tstatement.setString(3,id);\r\n\t\t\t    \tstatement.executeQuery();\r\n\t\t\t    \t\r\n\t\t\t\t}\r\n\t\t\t \r\n\t\t\t\t}\r\n\t\t\t\t\r\n            catch(Exception e)\r\n            {\r\n            System.out.println(\"exception \"+e.getMessage());\r\n            }\r\n            \r\n            finally\r\n            {\r\n            if(stmt!=null)\r\n            {\r\n                stmt.close();\r\n                statement.close();\r\n            }\r\n            if(connection!=null)\r\n            {\r\n                connection.close();\r\n            }\r\n            if(statement!=null)\r\n            {\r\n                \r\n                statement.close();\r\n            }\r\n             }\r\n             \r\n}\r\nupdatefile();\r\n"}
