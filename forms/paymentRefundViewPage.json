                "autoHandleWorkflowVariable": "true",
                "cacheInterval": "",
                "script": "import java.io.IOException;\r\nimport java.lang.String;\r\nimport org.apache.http.HttpResponse;\r\nimport org.apache.http.client.HttpClient;\r\nimport org.apache.http.client.config.CookieSpecs;\r\nimport org.apache.http.client.config.RequestConfig;\r\nimport org.apache.http.client.methods.HttpGet;\r\nimport org.apache.http.client.methods.HttpPost;\r\nimport org.apache.http.entity.StringEntity;\r\nimport org.apache.http.impl.client.HttpClients;\r\nimport org.json.JSONObject;\r\nimport java.sql.*;\r\nimport java.util.Date;\r\nimport java.sql.Connection;\r\nimport java.sql.PreparedStatement;\r\nimport org.apache.http.util.EntityUtils;\r\nimport java.text.SimpleDateFormat;\r\n\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport org.joget.apps.form.model.Element;\r\nimport org.joget.apps.form.model.FormData;\r\nimport org.joget.apps.form.model.FormRow;\r\nimport org.joget.apps.form.model.FormRowSet;\r\nimport org.joget.apps.form.service.FormUtil;\r\nimport org.joget.plugin.base.PluginManager;\r\nimport org.joget.apps.form.model.FormLoadBinder;\r\nimport org.joget.apps.form.model.FormStoreBinder;\r\n\r\n\r\nString orderReference = \"#form.app_fd_ofc_serReqs_tab.orderReference#\";\r\n\r\n\t// here we are update the order status\r\n\tpublic  updateRefundStatus(String status, String refundInitateDate) {\r\n     \r\n        \r\n        \r\n\t//\tSystem.out.println(\"status = \" + status);\r\n\t\t//System.out.println(\"refundInitateDate = \" + refundInitateDate);\r\n\t//\tSystem.out.println(\"orderReference = \" + orderReference);\r\n\t\t\r\n\t\t//Here we are getting Remarks TextArea value\r\n\t    PluginManager pluginManager = (PluginManager) AppUtil.getApplicationContext().getBean(\"pluginManager\");\r\n        FormStoreBinder binder = (FormStoreBinder) pluginManager.getPlugin(\"org.joget.apps.form.lib.WorkflowFormBinder\");\r\n        binder.store(element, rows, formData);\r\n        FormRow row = rows.get(0);\r\n        String refundRemarks = row.getProperty(\"refundRemarks\");\r\n       // System.out.println(\"RefundRemarks = \" + refundRemarks);\r\n\t\t\r\n\t\ttry {\r\n\r\n\t\t\tSimpleDateFormat formatter = new SimpleDateFormat(\"yyyy-MM-dd HH:mm:ss\");\r\n\t\t\tDate date = new Date();\r\n\t\t//\tSystem.out.println(\"Record dateModified Date========================================\");\r\n\t\t\tString dateModified = formatter.format(date).toString();\r\n\t\t\t//System.out.println(\"dateModified \" + dateModified);\r\n\r\n\t\t\tClass.forName(\"com.mysql.jdbc.Driver\").newInstance();\r\n\t\t\tConnection con = DriverManager.getConnection(\"jdbc:mysql://localhost:3307/jwdb?characterEncoding=UTF-8\",\"root\", \"\");\r\n\t\t\tif (!con.isClosed()) {\r\n\r\n\t\t\t\tString refundComplete = \"Refund Initiated\";\r\n\t\t\t\tString noRefund = \"No Refund\";\r\n\r\n\t\t\t\t// create the java mysql update preparedstatement c_bearer_access_token\r\n\t\t\t\tString query = \"update app_fd_ofc_serreqs_tab set dateModified = ? ,c_refund_status = ?,c_refund_initiate_date =?, c_refundRemarks = ? where c_orderReference = ?\";\r\n\t\t\t\tPreparedStatement preparedStmt = con.prepareStatement(query);\r\n\r\n\t\t\t\tif (status.equalsIgnoreCase(\"SUCCESS\")) {\r\n\t\t\t\t//\tSystem.out.println(\"Order refundComplete SUCCESS============\");\r\n\t\t\t\t\tpreparedStmt.setString(1, dateModified);\r\n\t\t\t\t\tpreparedStmt.setString(2, refundComplete);\r\n\t\t\t\t\tpreparedStmt.setString(3, refundInitateDate);\r\n\t\t\t\t\tpreparedStmt.setString(4, refundRemarks);\r\n\t\t\t\t} else {\r\n\t\t\t\t//\tSystem.out.println(\"noRefund ============\");\r\n\t\t\t\t\tpreparedStmt.setString(1, dateModified);\r\n\t\t\t\t\tpreparedStmt.setString(2, noRefund);\r\n\t\t\t\t\tpreparedStmt.setString(3, \"\");\r\n\t\t\t\t\tpreparedStmt.setString(4, \"\");\r\n\r\n\t\t\t\t}\r\n\t\t\t\tpreparedStmt.setString(5, orderReference);\r\n\t\t\t\t// execute the java PreparedStatement\r\n\t\t\t\tpreparedStmt.executeUpdate();\r\n\r\n\t\t\t\tcon.close();\r\n\t\t\t} else {\r\n\t\t\t\tSystem.out.println(\"Connection Problem\");\r\n\t\t\t}\r\n\t\t} catch (Exception ex) {\r\n\t\t\tex.printStackTrace();\r\n\r\n\t\t} finally {\r\n\r\n\t\t\tString redirectUrl = \"http://localhost:8080/jw/web/userview/ofc_app/v/_/serReqForm_crud\";\r\n\t\t\ttry {\r\n\t\t\t\tjava.awt.Desktop.getDesktop().browse(java.net.URI.create(redirectUrl));\r\n\t\t\t} catch (IOException e) {\r\n\t\t\t\t// TODO Auto-generated catch block\r\n\t\t\t\te.printStackTrace();\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\r\n// method for refund payment\r\n\tpublic  initiatePaymentRefund(String paymentRefundUrl, String access_token) {\r\n\r\n\t\tString amountStr = \"#form.ofc_ofc_category_tab.serReqTotAmt[{form.ofc_serReqs_tab.ofc_category_name}]#\";\r\n\t\tString finalAmount = \"\";\r\n\t\tint totalAmount = 0;\r\n\t\tint statusCode = 0;\r\n\r\n\t\t// here we are removing decimal points because create order payload only support\r\n\t\t// non-decimal amount\r\n\t\tif (!amountStr.equals(\"0.00\")) {\r\n\t\t\tString amountInDecimalString = amountStr;\r\n\t\t\tString amountWithoutDecimal = amountInDecimalString.replace(\".00\", \"\");\r\n\t\t\ttotalAmount = Integer.parseInt(amountWithoutDecimal);\r\n\t\t\ttotalAmount = totalAmount * 100;\r\n\t\t\tfinalAmount = Integer.toString(totalAmount);\r\n\r\n\t\t} else {\r\n\r\n\t\t\tfinalAmount = amountStr;\r\n\t\t}\r\n\r\n\t\tString jsonPayload = \"{\\\"amount\\\":{\\\"currencyCode\\\":\\\"AED\\\",\\\"value\\\":\" + finalAmount + \"}}\";\r\n\r\n\t\ttry {\r\n\t\t\t// Create the HttpClient and HttpPost objects\r\n\t\t\tHttpClient httpClient = HttpClients.custom()\r\n\t\t\t\t\t.setDefaultRequestConfig(RequestConfig.custom().setCookieSpec(CookieSpecs.STANDARD).build())\r\n\t\t\t\t\t.build();\r\n\t\t\tHttpPost httpPost = new HttpPost(paymentRefundUrl);\r\n\r\n\t\t\t// Set the request headers\r\n\t\t\thttpPost.setHeader(\"Authorization\", access_token);\r\n\t\t\thttpPost.setHeader(\"Content-Type\", \"application/vnd.ni-payment.v2+json\");\r\n\t\t\thttpPost.setHeader(\"Accept\", \"application/vnd.ni-payment.v2+json\");\r\n\r\n\t\t\t// Set the request body\r\n\t\t\tStringEntity requestEntity = new StringEntity(jsonPayload);\r\n\t\t\thttpPost.setEntity(requestEntity);\r\n\r\n\t\t\t// Send the request and receive the response\r\n\t\t\tHttpResponse response = httpClient.execute(httpPost);\r\n\r\n\t\t\t// Set the request body\r\n\t\t\tstatusCode = response.getStatusLine().getStatusCode();\r\n\t\t\tString responseBody = EntityUtils.toString(response.getEntity());\r\n\r\n\t\t\t// Print the response status code and body\r\n\t\t\t//System.out.println(\"Response Status Code: \" + statusCode);\r\n\t\t//\tSystem.out.println(\"Response Body: \" + responseBody);\r\n\r\n\t\t\tJSONObject refundResponceObject = new JSONObject(responseBody);\r\n\t\t\tString status = refundResponceObject.getJSONObject(\"_embedded\").getJSONArray(\"cnp:refund\").getJSONObject(0)\r\n\t\t\t\t\t.getString(\"state\");\r\n\t\t\tString refundInitateDate = refundResponceObject.getJSONObject(\"_embedded\").getJSONArray(\"cnp:refund\")\r\n\t\t\t\t\t.getJSONObject(0).getString(\"createdTime\");\r\n\r\n\t\t\t// call method for update refund status in db\r\n\t\t//\tSystem.out.println(\"status   =================\" + status);\r\n\t\t//\tSystem.out.println(\"refundInitateDate   =================\" + refundInitateDate);\r\n\r\n\t\t\t// calling initiatePaymentRefund() method for update refund details in database\r\n\t\t\tupdateRefundStatus(status, refundInitateDate);\r\n\r\n\t\t} catch (Exception e) {\r\n\t\t\tSystem.out.println(\"initiatePaymentRefund()  Response Status Code: \" + statusCode);\r\n\t\t\te.printStackTrace();\r\n\t\t}\r\n\r\n\t}\r\n\t\r\n\t// Here we are creating request for order status retrieve API\r\n\tpublic   getOrderStatus(String access_token) {\r\n\r\n\t\tString outletId = \"a2c3b529-68c7-44eb-8a2a-3aeb12d83ae6/orders\";\r\n\t\tString orderStausUrl = \"https://api-gateway.sandbox.ngenius-payments.com/transactions/outlets/\" + outletId + \"/\"\r\n\t\t\t\t+ orderReference;\r\n\r\n\t\ttry {\r\n\t\t\t// Create the HttpClient and HttpPost objects\r\n\t\t\tHttpClient httpClient = HttpClients.createDefault();\r\n\t\t\tHttpGet httpGet = new HttpGet(orderStausUrl);\r\n\r\n\t\t\t// set headers\r\n\t\t\thttpGet.setHeader(\"Authorization\", access_token);\r\n\t\t\thttpGet.setHeader(\"Content-Type\", \"application/vnd.ni-payment.v2+json\");\r\n\t\t\thttpGet.setHeader(\"Accept\", \"application/vnd.ni-payment.v2+json\");\r\n\r\n\t\t\t// Send the request and receive the response\r\n\t\t\tHttpResponse response = httpClient.execute(httpGet);\r\n\r\n\t\t\tint statusCode = response.getStatusLine().getStatusCode();\r\n\t\t\tString responseBody = EntityUtils.toString(response.getEntity());\r\n\r\n\t\t\t//System.out.println(\"Refund Response Status Code: \" + statusCode);\r\n\t\t\t//System.out.println(\"Refund Response Body: \" + responseBody);\r\n\r\n\t\t\t// here we are getting payment refund Link\r\n\t\t\tJSONObject refundResponceObject = new JSONObject(responseBody);\r\n\t\t\tString paymentRefundUrl = refundResponceObject.getJSONObject(\"_embedded\").getJSONArray(\"payment\")\r\n\t\t\t\t\t.getJSONObject(0).getJSONObject(\"_links\").getJSONObject(\"cnp:refund\").getString(\"href\");\r\n\t\t\t//System.out.println(\"paymentRefundUrl \" + paymentRefundUrl);\r\n\r\n\t\t\t// call refund api\r\n\t\t\tinitiatePaymentRefund(paymentRefundUrl, access_token);\r\n\r\n\t\t} catch (IOException e) {\r\n\t\t\t//System.out.println(\"Payment Refund Redirect to Payment not settled Form\");\r\n\t\t\tString redirectUrl = \"http://localhost:8080/jw/web/userview/ofc_app/v/_/paymentNotSettledForm\";\r\n\t\t\ttry {\r\n\t\t\t\tjava.awt.Desktop.getDesktop().browse(java.net.URI.create(redirectUrl));\r\n\t\t\t} catch (IOException e1) {\r\n\t\t\t\t// TODO Auto-generated catch block\r\n\t\t\t\te1.printStackTrace();\r\n\t\t\t}\r\n\r\n\t\t\te.printStackTrace();\r\n\t\t}\r\n\r\n\t}\r\n\r\n// Here we are getting the access token\r\n\tpublic void getAccessToken() {\r\n\r\n\t\tint statusCode = 0;\r\n\t\ttry {\r\n\r\n\t\t\tString url = \"https://api-gateway.sandbox.ngenius-payments.com/identity/auth/access-token\";\r\n\t\t\tString authorization = \"Authorization\";\r\n\t\t\tString authorizationValue = \"Basic OGQwZWZiNTQtMmQ0YS00YmNlLTg5ODMtMTczMGEyZmRkOGU3OjQzNzNhMTFmLTMxYTgtNGE5NC05OWU5LWI4Y2FhYTI0MTJjZQ==\";\r\n\t\t\tString contentType = \"Content-Type\";\r\n\t\t\tString contentTypeValue = \"application/vnd.ni-identity.v1+json\";\r\n\r\n\t\t\t// Create the HttpClient and HttpPost objects and code for remove cookie warring\r\n\t\t\tHttpClient httpClient = HttpClients.custom()\r\n\t\t\t\t\t.setDefaultRequestConfig(RequestConfig.custom().setCookieSpec(CookieSpecs.STANDARD).build())\r\n\t\t\t\t\t.build();\r\n\t\t\tHttpPost httpPost = new HttpPost(url);\r\n\r\n\t\t\t// Set the request headers\r\n\t\t\thttpPost.setHeader(authorization, authorizationValue);\r\n\t\t\thttpPost.setHeader(contentType, contentTypeValue);\r\n\r\n\t\t\t// Send the request and receive the response\r\n\t\t\tHttpResponse response = httpClient.execute(httpPost);\r\n\r\n\t\t\t// Get the response status code and body\r\n\t\t\tstatusCode = response.getStatusLine().getStatusCode();\r\n\t\t\tString responseBody = EntityUtils.toString(response.getEntity());\r\n\r\n\t\t\t// getting the order access token\r\n\t\t\tJSONObject jsonData = new JSONObject(responseBody);\r\n\t\t\tString access_token = (String) jsonData.get(\"access_token\");\r\n\r\n\t\t\t//System.out.println(\"access_token - \" + access_token);\r\n\r\n\t\t\t// Print the response status code and body\r\n\t\t\t//System.out.println(\"getAccessToken() Response Status Code: \" + statusCode);\r\n\t\t\t//System.out.println(\"getAccessToken() Response Body: \" + responseBody);\r\n\t\t\t//System.out.println(\"getAccessToken() access_token Body: \" + access_token);\r\n\r\n\t\t\t// calling get\r\n\t\t\tgetOrderStatus(\"Bearer \" + access_token);\r\n\r\n\t\t} catch (Exception ex) {\r\n\r\n\t\t\t//System.out.println(\"getAccessToken() Status code : \" + statusCode);\r\n\t\t\t//System.out.println(\"Payment Refund Redirect to Payment not settled Form\");\r\n\t\t\tString redirectUrl = \"http://localhost:8080/jw/web/userview/ofc_app/v/_/paymentNotSettledForm\";\r\n\t\t\ttry {\r\n\t\t\t\tjava.awt.Desktop.getDesktop().browse(java.net.URI.create(redirectUrl));\r\n\t\t\t} catch (IOException e1) {\r\n\t\t\t\t// TODO Auto-generated catch block\r\n\t\t\t\te1.printStackTrace();\r\n\t\t\t}\r\n\t\t\tex.printStackTrace();\r\n\r\n\t\t}\r\n\r\n\t}\r\n\t// method calling\r\n getAccessToken();\t",
