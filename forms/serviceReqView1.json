                "script": "import java.io.BufferedReader;\r\nimport java.io.IOException;\r\nimport java.io.InputStreamReader;\r\n\r\nimport org.apache.http.HttpResponse;\r\nimport org.apache.http.client.config.CookieSpecs;\r\nimport org.apache.http.client.config.RequestConfig;\r\nimport org.apache.http.client.methods.HttpPost;\r\nimport org.apache.http.client.methods.HttpRequestBase;\r\nimport org.apache.http.impl.client.CloseableHttpClient;\r\nimport org.apache.http.impl.client.HttpClients;\r\nimport org.json.JSONObject;\r\n\r\n\r\nimport okhttp3.MediaType;\r\nimport okhttp3.OkHttpClient;\r\nimport okhttp3.Request;\r\nimport okhttp3.RequestBody;\r\nimport okhttp3.Response;\r\nimport org.joget.commons.util.LogUtil;\r\n\r\n\r\n\r\nOkHttpClient okHttpClient = new OkHttpClient();\r\nString access_token =null;\r\n\tpublic  getAccessToken() {\r\n\r\n\r\n\r\n\t\t\tHttpRequestBase request = null;\r\n\t\t\tCloseableHttpClient client = null;\r\n\t\t\tHttpResponse response =null;\r\n\t\t\ttry {\r\n\r\n\t\t\t\tString jsonUrl = \"https://api-gateway.sandbox.ngenius-payments.com/identity/auth/access-token\";\r\n\t\t\t\tString authorization = \"Authorization\";\r\n\t\t\t\tString authorizationValue = \"Basic OGQwZWZiNTQtMmQ0YS00YmNlLTg5ODMtMTczMGEyZmRkOGU3OjQzNzNhMTFmLTMxYTgtNGE5NC05OWU5LWI4Y2FhYTI0MTJjZQ==\";\r\n\t\t\t\tString contentType = \"Content-Type\";\r\n\t\t\t\tString contentTypeValue = \"application/vnd.ni-identity.v1+json\";\r\n                    \r\n                    //code for remove cookie warring\r\n\t\t        \tclient = HttpClients.custom()\r\n\t\t\t\t\t.setDefaultRequestConfig(RequestConfig.custom().setCookieSpec(CookieSpecs.STANDARD).build())\r\n\t\t\t\t\t.build();\r\n\t\t\t\t\t\r\n\t\t\t\trequest = new HttpPost(jsonUrl);\r\n\t\t\t\trequest.setHeader(authorization, authorizationValue);\r\n\t\t\t\trequest.setHeader(contentType, contentTypeValue);\r\n\t\t\t\t// request.setHeader(\"header1\", \"value1\");//test\r\n\t\t\t\tresponse = client.execute(request);\r\n\r\n\t\t\t\t\r\n\t\t\t\tif (response.getStatusLine().getStatusCode() != 200) {\r\n\t\t\t\t\tthrow new RuntimeException(\"Failed : HTTP error code : \" + response.getStatusLine().getStatusCode());\r\n\t\t\t\t}\r\n\r\n\t\t\t\tBufferedReader br = new BufferedReader(new InputStreamReader((response.getEntity().getContent())));\r\n\r\n\t\t\t \r\n\t\t\t\tString output = null;\r\n\t\t\t\tString dataString = \"\";\r\n\t\t\t\tSystem.out.println(\"Output from Server .... \\n\");\r\n\t\t\t\twhile ((output = br.readLine()) != null) {\r\n\t\t\t\t      dataString += output;\r\n\t\t\t\t  //  LogUtil.info(\"accessToken\", output);\r\n\t\t\t\t//\tSystem.out.println(output);\r\n\t\t\t\t \r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t\t// get access token\r\n\t\t\t\tJSONObject json = new JSONObject(dataString);\r\n\t\t\t\t access_token = json.getString(\"access_token\");\r\n\t\t\t\t\r\n\t\t\t\r\n\t\t\t\t///LogUtil.info(\"accessToken :\", access_token);\r\n\t\t\t\t//LogUtil.info(\"createOrderApi  Execution\", \"Start\");\r\n\t\t\t\r\n\t\t       \r\n\t\t\t\t\t//System.out.println(\"Create ORDER :\"+ response1.body().string()) ;\r\n\t\t\t\t\r\n\t\t\t\t\t//Create Order Api call\r\n\t\t \r\n\t\t\t\t\r\n\t\t\t} catch (Exception ex) {\r\n\r\n\t\t\t\t// LogUtil.error(getClass().getName(), ex, \"\");\r\n\t\t\t\t// formRow.put(\"textField\", \"catch mian ex\"+getClass().getName());\r\n\t\t\t\t// formRowSet.add(formRow);\r\n\r\n\t\t\t} finally {\r\n\t\t\t\ttry {\r\n\t\t\t\t\tif (request != null) {\r\n\t\t\t\t\t\trequest.releaseConnection();\r\n\t\t\t\t\t}\r\n\t\t\t\t\tif (client != null) {\r\n\t\t\t\t\t\tclient.close();\r\n\t\t\t\t\t}\r\n\t\t\t\t} catch (IOException ex) {\r\n\t\t\t\t\t// LogUtil.info(\"accessToken\", ex, \"finally try catch error\");\r\n\r\n\t\t\t\t}\r\n\t\t\t\t// return formRowSet;\r\n\t\t\t}\r\n\t\t\r\n\r\n\t}\r\n\t\r\n\t\r\n\t\r\ngetAccessToken();\r\n\r\n \tpublic   createOrder(){\r\n\t\tString json2 = \"{\\\"action\\\":\\\"PURCHASE\\\",\\\"amount\\\":{\\\"currencyCode\\\":\\\"AED\\\",\\\"value\\\":200},\\\"merchantOrderReference\\\":\\\"MID\\\",\\\"merchantAttributes\\\":{\\\"redirectUrl\\\":\\\"https://google.com\\\",\\\"skipConformationPage\\\":\\\"true\\\",\\\"cancelText\\\":\\\"ContinueShopping\\\",\\\"cancelUrl\\\":\\\"https://google.com\\\"},\\\"emailAddress\\\":\\\"test@gmail.com\\\",\\\"billingAddress\\\":{\\\"firstName\\\":\\\"Sagar\\\",\\\"lastName\\\":\\\"Gupta\\\",\\\"address1\\\":\\\"ShivColony\\\",\\\"city\\\":\\\"Gwalior\\\",\\\"countryCode\\\":\\\"India\\\"}}\";\r\n\t\tString url = \"https://api-gateway.sandbox.ngenius-payments.com/transactions/outlets/a2c3b529-68c7-44eb-8a2a-3aeb12d83ae6/orders\";\r\n\t\tMediaType JSON = MediaType.parse(\"application/vnd.ni-payment.v2+json\");\r\n\t\tRequestBody body = RequestBody.create(JSON, json2);\r\n\t\tRequest request1 = new Request.Builder().url(url).post(body)\r\n\t\t\t\t.addHeader(\"Authorization\", \"Bearer \" + access_token).build();\r\n\r\n\t\tString createOrderResponse = null;\r\n\t\tString paymentLink = null;\r\n\t\ttry {\r\n\t\t\tResponse response1 = okHttpClient.newCall(request1).execute();\r\n\t\t\tcreateOrderResponse = response1.body().string();\r\n\t\t\t// LogUtil.info(\"Create ORDER -:\", response1.body().string()) ;\r\n\r\n\t\t\tSystem.out.println(\"Create ORDER -:\" + createOrderResponse);\r\n\t\t\tJSONObject json = new JSONObject(createOrderResponse);\r\n\r\n\t\t\tJSONObject links = json.getJSONObject(\"_links\");\r\n\t\t\tJSONObject payment = links.getJSONObject(\"payment\");\r\n\t\t\tpaymentLink = payment.getString(\"href\");\r\n            \r\n            //Open payment link\r\n        \tString url_open = paymentLink;\r\n        \tjava.awt.Desktop.getDesktop().browse(java.net.URI.create(url_open));\r\n\t\t\tLogUtil.info(\"Payment Link -:\" , paymentLink);\r\n\t  \r\n\t\t\r\n\t      String userEmail =  \"#form.ofc_serReqs_tab.serReqUserName#\";\r\n\t      \r\n\t      String totalAmount  =  \"#form.ofc_company_tab.ownerFirstName[{form.ofc_serReqs_tab.ownerFirstName}]#\" ;\r\n \t      LogUtil.info(\"totalAmount -:\" , totalAmount);\r\n\t\t\r\n          \r\n\r\n\t\t} catch (IOException e) {\r\n\t\t\t// TODO Auto-generated catch block\r\n\t\t\te.printStackTrace();\r\n\t\t}\r\n         \r\n\t}\r\n\r\ncreateOrder();\r\n",
