                "script": "import java.io.BufferedReader;\r\nimport java.io.IOException;\r\nimport java.io.InputStreamReader;\r\nimport java.lang.String;\r\nimport org.apache.http.HttpResponse;\r\nimport org.apache.http.client.config.CookieSpecs;\r\nimport org.apache.http.client.config.RequestConfig;\r\nimport org.apache.http.client.methods.HttpPost;\r\nimport org.apache.http.client.methods.HttpRequestBase;\r\nimport org.apache.http.impl.client.CloseableHttpClient;\r\nimport org.apache.http.impl.client.HttpClients;\r\nimport org.json.JSONObject;\r\nimport okhttp3.MediaType;\r\nimport okhttp3.OkHttpClient;\r\nimport okhttp3.Request;\r\nimport okhttp3.RequestBody;\r\nimport okhttp3.Response;\r\nimport org.joget.commons.util.LogUtil;\r\nimport org.joget.apps.form.model.*;\r\nimport org.joget.apps.form.service.*;\r\nimport java.sql.*;\r\nimport java.util.*;\r\nimport org.apache.commons.collections.SequencedHashMap;\r\nimport java.sql.Connection;\r\nimport java.sql.PreparedStatement;\r\nimport java.sql.ResultSet;\r\nimport java.sql.SQLException;\r\nimport javax.sql.DataSource;\r\nimport org.joget.apps.app.model.AppDefinition;\r\nimport org.joget.apps.app.service.AppService;\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport org.joget.apps.form.model.Element;\r\nimport org.joget.apps.form.model.FormData;\r\nimport org.joget.apps.form.model.FormRow;\r\nimport org.joget.apps.form.model.FormRowSet;\r\nimport org.joget.apps.form.model.FormStoreBinder;\r\nimport org.joget.apps.form.service.FormUtil;\r\nimport org.joget.plugin.base.PluginManager;\r\n\r\n\r\n\r\nOkHttpClient okHttpClient = new OkHttpClient();\r\nString access_token =null;\r\n//Here we are gatting the access token\r\npublic  getAccessToken() {\r\n\r\n  HttpRequestBase request = null;\r\n  HttpResponse response =null;\r\n  CloseableHttpClient client = null;\r\n  String output = null;\r\n  String dataString = \"\";\r\n  \r\ntry {\r\n\r\n\t\t\t\tString jsonUrl = \"https://api-gateway.sandbox.ngenius-payments.com/identity/auth/access-token\";\r\n\t\t\t\tString authorization = \"Authorization\";\r\n\t\t\t\tString authorizationValue = \"Basic OGQwZWZiNTQtMmQ0YS00YmNlLTg5ODMtMTczMGEyZmRkOGU3OjQzNzNhMTFmLTMxYTgtNGE5NC05OWU5LWI4Y2FhYTI0MTJjZQ==\";\r\n\t\t\t\tString contentType = \"Content-Type\";\r\n\t\t\t\tString contentTypeValue = \"application/vnd.ni-identity.v1+json\";\r\n                    \r\n                    //code for remove cookie warring\r\n\t\t        \tclient = HttpClients.custom()\r\n\t\t\t\t\t.setDefaultRequestConfig(RequestConfig.custom().setCookieSpec(CookieSpecs.STANDARD).build())\r\n\t\t\t\t\t.build();\r\n\t\t\t\t\t\r\n\t\t\t\trequest = new HttpPost(jsonUrl);\r\n\t\t\t\trequest.setHeader(authorization, authorizationValue);\r\n\t\t\t\trequest.setHeader(contentType, contentTypeValue);\r\n\t\t\t\tresponse = client.execute(request);\r\n\t\t\t\t\r\n\t\t\t\tif (response.getStatusLine().getStatusCode() != 200) {\r\n\t\t\t\t\tthrow new RuntimeException(\"Failed : HTTP error code : \" + response.getStatusLine().getStatusCode());\r\n\t\t\t\t}\r\n\t\t\t\tBufferedReader br = new BufferedReader(new InputStreamReader((response.getEntity().getContent())));\r\n\r\n\twhile ((output = br.readLine()) != null) {\r\n\t\t   \r\n\t\t    dataString += output;\r\n      \t//LogUtil.info(\"accessToken\", output);\r\n\t\t}\r\n\t\t// get access token\r\n\t\tJSONObject json = new JSONObject(dataString);\r\n\t\taccess_token = json.getString(\"access_token\");\r\n\r\n\t} catch (Exception ex) {\r\n       \tex.printStackTrace();\r\n\r\n\t\t\t} finally {\r\n\t\t\t\ttry {\r\n\t\t\t\t\tif (request != null) {\r\n\t\t\t\t\t\trequest.releaseConnection();\r\n\t\t\t\t\t}\r\n\t\t\t\t\tif (client != null) {\r\n\t\t\t\t\t\tclient.close();\r\n\t\t\t\t\t}\r\n\t\t\t\t} catch (IOException ex) {\r\n\t\t\t\t\tex.printStackTrace();\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t}\r\n//method calling\t\r\ngetAccessToken();\r\n\r\n// Here we are saving the order details in db\r\npublic   setOrderDetailsInDB(String orderReference, String merchantOrderReference,String access_token) {\r\n\r\n    try {\r\n\t\t\tClass.forName(\"com.mysql.jdbc.Driver\").newInstance();\r\n\t\t\tConnection con = DriverManager.getConnection(\"jdbc:mysql://localhost:3307/jwdb?characterEncoding=UTF-8\",\"root\", \"\");\r\n\t\t\tif (!con.isClosed()) {\r\n\t\t\t    \r\n                 String id= \"#form.ofc_serReqs_tab.id#\";\r\n\t\t\t\t// create the java mysql update preparedstatement  c_bearer_access_token\r\n\t\t\t\tString query = \"update app_fd_ofc_serreqs_tab set c_merchantOrderReference = ?, c_orderReference = ?, c_bearer_access_token = ?  where id = ?\";\r\n\t\t\t\tPreparedStatement preparedStmt = con.prepareStatement(query);\r\n\t\t\t\tpreparedStmt.setString(1, merchantOrderReference);\r\n\t\t\t\tpreparedStmt.setString(2, orderReference);\r\n\t\t\t\tpreparedStmt.setString(3, \"Bearer \"+access_token);\r\n\t\t\t\tpreparedStmt.setString(4, id);\r\n\r\n\t\t\t\t// execute the java preparedstatement\r\n\t\t\t\tpreparedStmt.executeUpdate();\r\n\r\n\t\t\t\tcon.close();\r\n\t\t\t} else {\r\n\t\t\t\tSystem.out.println(\"Connection Problem\");\r\n\t\t\t}\r\n\t\t} catch (Exception ex) {\r\n\t\t\tex.printStackTrace();\r\n\r\n\t\t}\r\n    \r\n    \r\n   \r\n}\r\n  \r\n\r\n//Here we are creating order\r\npublic createOrder(){\r\n      \r\n        // Local urls\r\n        String cancelUrl =   \"http://127.0.0.1:8080/jw/web/userview/ofc_app/v/_/serReqForm_crud\";\r\n        String redirectUrl = \"http://127.0.0.1:8080/jw/web/userview/ofc_app/v/_/paymentRedirectPage?url=%2Fjw%2Fweb%2Fuserview%2Fofc_app%2Fv%2F_%2FpaymentRedirectPage\";\r\n       //Cloud urls\r\n         //String cancelUrl =   \"https://concordia.cloud.joget.com/jw/web/userview/ofc_app/v/_/serReqForm_crud\";\r\n         //String redirectUrl = \"https://concordia.cloud.joget.com/jw/web/userview/ofc_app/v/_/paymentRedirectPage?url=%2Fjw%2Fweb%2Fuserview%2Fofc_app%2Fv%2F_%2FpaymentRedirectPage\";\r\n       \r\n        String createOrderurl = \"https://api-gateway.sandbox.ngenius-payments.com/transactions/outlets/a2c3b529-68c7-44eb-8a2a-3aeb12d83ae6/orders\";\r\n      \tint totalAmount=0;\r\n      \tString finalAmount =\"\";\r\n      //getting data for payment request form process workflow (Service Request View) \r\n\t    String  amountStr = \"#form.ofc_ofc_category_tab.serReqTotAmt[{form.ofc_serReqs_tab.ofc_category_name}]#\";\r\n\t   if(!amountStr.equals(\"0.00\")) {\r\n\t\t\t \r\n\t    String amountInDecimalString = amountStr;\r\n\t\tString amountWithoutDecimal = amountInDecimalString.replace(\".00\", \"\");\r\n     \ttotalAmount = Integer.parseInt(amountWithoutDecimal);\r\n\t\ttotalAmount = totalAmount * 100;\r\n\t\t  finalAmount = Integer.toString(totalAmount);\r\n\t\t\r\n\t\t }else{\r\n\t\t     \r\n\t\t      finalAmount = amountStr;\r\n\t\t }\r\n\t  \r\n\t\t\r\n\t   //String  totalAmount = amountStr.replace(\".00\",\"\");\r\n\t   String  companyNameUsername = \"#form.ofc_company_tab.companyName[{form.ofc_serReqs_tab.companyName}]#\";\r\n\t   String  companyNamOrEmail = \"#form.ofc_serReqs_tab.serReqUserName#\";\r\n\t   String  dmccNo = \"#form.ofc_serReqs_tab.serReqDmccSrNo#\";\r\n\t   String  createOrderResponse = null;\r\n\t   String  paymentLink = null;\r\n\t   //randomly generates a UUID for merchantOrderReference\r\n       UUID uuid = UUID.randomUUID();  \r\n       String merchantOrderReference = uuid.toString();\r\n       System.out.println(\"Randomly Generated UUID: \"+uuid);    \r\n\t  //LogUtil.info(\"companyNamOrEmail \", companyNamOrEmail);\r\n\t // LogUtil.info(\"totalAmount \", totalAmount);\r\n\t // LogUtil.info(\"dmccNo \", dmccNo);\r\n\t  //LogUtil.info(\"companyNameUsername \", companyNameUsername);\r\n\t   \t  \t  \r\n\t// Create order Request \r\n    String createOrderJsonBody = \"{\\\"action\\\":\\\"PURCHASE\\\",\\\"amount\\\":{\\\"currencyCode\\\":\\\"AED\\\",\\\"value\\\":\"+finalAmount+\"},\\\"merchantOrderReference\\\":\\\"\"+merchantOrderReference+\"\\\",\\\"merchantAttributes\\\":{\\\"redirectUrl\\\":\\\"\"+redirectUrl+\"\\\",\\\"skipConfirmationPage\\\":\\\"true\\\",\\\"cancelText\\\":\\\"GoBacktoApplication\\\",\\\"cancelUrl\\\":\\\"\"+cancelUrl+\"\\\"},\\\"emailAddress\\\":\\\"\"+companyNamOrEmail+\"\\\",\\\"billingAddress\\\":{\\\"firstName\\\":\\\"\"+companyNameUsername+\"\\\",\\\"lastName\\\":\\\"Kumar\\\",\\\"address1\\\":\\\"Dubai\\\",\\\"city\\\":\\\"Dubai\\\",\\\"countryCode\\\":\\\"UAE\\\"}}\";\r\n\t \r\n\t\tMediaType JSON = MediaType.parse(\"application/vnd.ni-payment.v2+json\");\r\n\t\tRequestBody body = RequestBody.create(JSON, createOrderJsonBody);\r\n\t\tRequest request1 = new Request.Builder().url(createOrderurl).post(body)\r\n\t\t\t\t.addHeader(\"Authorization\", \"Bearer \" + access_token).build();\r\n\t\ttry {\r\n\t\t\tResponse response1 = okHttpClient.newCall(request1).execute();\r\n\t\t\tcreateOrderResponse = response1.body().string();\r\n\t\tSystem.out.println(\"createOrderResponse :\"+createOrderResponse);\r\n\t\t     JSONObject jsonData = new JSONObject(createOrderResponse);\r\n\t        String  orderReference  = (String) jsonData.get(\"reference\");\r\n\t\t   System.out.println(\"orderReference \"+ orderReference);\r\n\t\t  //calling order details save method\r\n\t\t   setOrderDetailsInDB(orderReference,merchantOrderReference,access_token);\r\n\t \r\n\t\t\tJSONObject json = new JSONObject(createOrderResponse);\r\n\t\t\t// getting payment link from response\r\n\t\t\tJSONObject links = json.getJSONObject(\"_links\");\r\n\t\t\tJSONObject payment = links.getJSONObject(\"payment\");\r\n\t\t\tpaymentLink = payment.getString(\"href\");\r\n\t\t\t// Execute payment link\r\n\t\t\tjava.awt.Desktop.getDesktop().browse(java.net.URI.create(paymentLink));\r\n\r\n\t\t} catch (IOException e) {\r\n\t\t\t \r\n\t\t\te.printStackTrace();\r\n\t\t}\r\n\t}\r\n\r\n//calling creating order method\r\ncreateOrder();\r\n",
