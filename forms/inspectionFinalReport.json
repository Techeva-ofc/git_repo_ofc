                "cacheInterval": "",
                "script": "import org.joget.apps.form.model.*;\r\nimport org.joget.apps.form.service.*;\r\nimport java.sql.*;\r\nimport java.util.*;\r\nimport org.apache.commons.collections.SequencedHashMap;\r\nimport java.sql.Connection;\r\nimport java.sql.PreparedStatement;\r\nimport java.sql.ResultSet;\r\nimport java.sql.SQLException;\r\nimport javax.sql.DataSource;\r\nimport org.joget.apps.app.model.AppDefinition;\r\nimport org.joget.apps.app.service.AppService;\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport org.joget.apps.form.model.Element;\r\nimport org.joget.apps.form.model.FormData;\r\nimport org.joget.apps.form.model.FormRow;\r\nimport org.joget.apps.form.model.FormRowSet;\r\nimport org.joget.apps.form.model.FormStoreBinder;\r\nimport org.joget.apps.form.service.FormUtil;\r\nimport org.joget.plugin.base.PluginManager;\r\nimport org.joget.commons.util.LogUtil;\r\npublic FormRowSet findLast_seq_no() \r\n{\r\nFormRowSet formRowSet = new FormRowSet();\r\nformRowSet.setMultiRow(true);\r\nClass.forName(\"com.mysql.jdbc.Driver\").newInstance();\r\nConnection con = DriverManager.getConnection(\"jdbc:mysql://localhost:3307/jwdb?characterEncoding=UTF-8\", \"root\", \"\");\r\nif (!con.isClosed()) \r\n{\r\n    String serReqDmccSrNo = \"#form.app_fd_ofc_serreqs_tab.serReqDmccSrNo#\";\r\n    String query = \"select dateModified,c_serReqDmccSrNo,c_companyName from app_fd_ofc_serreqs_tab where c_serReqDmccSrNo='786565'\";\r\n    PreparedStatement preparedStmt = con.prepareStatement(query);\r\n    ResultSet resultSet = preparedStmt.executeQuery();\r\n    System.out.println(\"connection open\");\r\n    System.out.println(preparedStmt);\r\n    \r\n    \r\n    String compnayNameId = null;\r\n    while (resultSet.next()) \r\n    {\r\n        formRow.put(\"licensenumber\",resultSet.getString(\"c_serReqDmccSrNo\"));\r\n        dmcc = resultSet.getString(\"c_serReqDmccSrNo\");\r\n        compnayNameId = resultSet.getString(\"c_companyName\");\r\n        formRow.put(\"date\",resultSet.getString(\"dateModified\"));\r\n    }\r\n    System.out.println(\"---------------------------------\"+compnayNameId+\"----------\"+dmcc);\r\n    // 'd31e64c8-4e93-4779-8746-4d630ec1d04f'\r\n    String queryCompnayTable = \"select c_companyName, c_companyAddress, c_compActivity from app_fd_ofc_company_tab where id= \\\"\"+ compnayNameId + \"\\\"\";\r\n    PreparedStatement preparedStmtCompnay = con.prepareStatement(queryCompnayTable);\r\n    ResultSet resultSetCompnay = preparedStmtCompnay.executeQuery();\r\n    String compnayName = null;\r\n    String address = null;\r\n    String compnayActivity = null;\r\n    while (resultSetCompnay.next()) \r\n    {\r\n        formRow.put(\"report_companyname\",resultSetCompnay.getString(\"c_companyName\"));\r\n        formRow.put(\"location\",resultSetCompnay.getString(\"c_companyAddress\"));\r\n        compnayActivity = resultSetCompnay.getString(\"c_compActivity\");\r\n    }\r\n    System.out.println(\"compnayActivity \" + compnayActivity);\r\n    String queryActivityTable = \"select c_activityName from app_fd_ofc_activity_tab where id= \\\"\"+ compnayActivity + \"\\\"\";\r\n    PreparedStatement preparedStmtCompnay = con.prepareStatement(queryCompnayTable);\r\n    ResultSet resultSetActivity = preparedStmtCompnay.executeQuery();\r\n    \r\n    while (resultSetCompnay.next()) \r\n    {\r\n        formRow.put(\"licenseactivity\",resultSetActivity.getString(\"c_activityName\"));\r\n        \r\n    }\r\n    System.out.println(compnayName);\r\n    System.out.println(address);\r\n\r\n\r\n        } else {\r\n    System.out.println(\"Connection Problem\");\r\n    }\r\n    con.close();\r\n    return formRowSet;\r\n}\r\nreturn findLast_seq_no();",
