tpreparedStmt.setString(2, serReqDmccSrNo);\r\n\t\t\t \r\n\t\t\t\t// execute the java preparedstatement\r\n\t\t\t\tpreparedStmt.executeUpdate();\r\n\r\n\t\t\t\tcon.close();\r\n\t\t\t} else {\r\n\t\t\t\tSystem.out.println(\"Connection Problem\");\r\n\t\t\t}\r\n\t\t} catch (Exception ex) {\r\n\t\t\tex.printStackTrace();\r\n\r\n\t\t}finally{\r\n\t\t//local url    \r\n\t\r\n      String redirectUrl = \"http://127.0.0.1:8080/jw/web/userview/ofc_app/v/_/ofc_inspection_form_crud\";\r\n       //cloud url\r\n      // String redirectUrl = \"https://concordia.cloud.joget.com/jw/web/userview/ofc_app/v/_/ofc_inspection_form_crud\";\r\n    \t// Execute payment link\r\n\t  java.awt.Desktop.getDesktop().browse(java.net.URI.create(redirectUrl));\r\n\t}\r\n}\r\n\t\r\n  updateinspectionScheduled();\t\r\n\t\r\n"}
