= \"#form.app_fd_ofc_serreqs_tab.image_by_client#\";\r\n                 String fees = null;\r\n                 \r\n                  System.out.println(\"serReqDmccSrNo ---------\"+reportStatus+\" iD \"+id);\r\n                 //String query = \"INSERT INTO app_fd_ofc_insp_report (id,c_serReqDmccSrNo, c_inspectionStatus) VALUES (?, ?,?)\";\r\n                 \r\n                 if(reportStatus.equals(\"Full Compliance\"))\r\n                 {\r\n                     inspectionStatus = \"Inspection Completed\";\r\n                 }\r\n                 else if(reportStatus.equals(\"Partial Compliance\"))\r\n                 {\r\n                     inspectionStatus = \"Image(s) To-Be Uploaded\";\r\n                 }\r\n                 else if(reportStatus.equals(\"Other Non-Compliance\"))\r\n                 {\r\n                     fees = \"500.00\";\r\n                     inspectionStatus = \"Inspection To-Be Re-Scheduled\";\r\n                 }\r\n                 else if(reportStatus.equals(\"Fitout Non-Compliance\"))\r\n                 {\r\n                     fees = \"5250.00\";\r\n                     inspectionStatus = \"Awaiting payment from the client\";\r\n                 }\r\n                 else\r\n                 {\r\n                     \r\n                 }\r\n                 \r\n                 String query = \"update app_fd_ofc_serreqs_tab set c_feetopay = ?, c_status = ? where c_serReqDmccSrNo = ? and id = ?\";\r\n                 \r\n                 PreparedStatement preparedStmt = con.prepareStatement(query);\r\n                 //preparedStmt.setString(1, id);\r\n                 preparedStmt.setString(1, fees);\r\n                 preparedStmt.setString(2, inspectionStatus);\r\n                 preparedStmt.setString(3, serReqDmccSrNo);\r\n                 preparedStmt.setString(4, id);// execute the java preparedstatement\r\n                 preparedStmt.executeUpdate();\r\n                 } else {\r\n                 System.out.println(\"Connection Problem\");\r\n             }\r\n         } catch (Exception ex) {\r\n             ex.printStackTrace(); \r\n             } finally {\r\n             // local url\r\n             try {\r\n                 con.close();\r\n             } catch (SQLException e) {\r\n                 // TODO Auto-generated catch block\r\n                 e.printStackTrace();\r\n             }\r\n             String redirectUrl = \"http://127.0.0.1:8080/jw/web/userview/ofc_app/v/_/reportSummary\";\r\n             // cloud url\r\n             // String redirectUrl =\r\n             // \"https://concordia.cloud.joget.com/jw/web/userview/ofc_app/v/_/ofc_inspection_form_crud\";\r\n             // Execute payment link\r\n             java.awt.Desktop.getDesktop().browse(java.net.URI.create(redirectUrl));\r\n         }\r\n    }\r\nupdateinspectionScheduled();\r\n"}
