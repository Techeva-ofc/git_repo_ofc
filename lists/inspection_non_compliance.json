            "rules": [],
            "script": "import java.util.ArrayList;\r\nimport java.util.List;\r\nimport org.joget.apps.app.model.AppDefinition;\r\nimport org.joget.apps.app.model.AppPlugin;\r\nimport org.joget.apps.app.service.AppPluginUtil;\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport org.joget.plugin.base.PluginProperty;\r\nimport org.joget.plugin.base.PluginWebSupport;\r\nimport java.lang.String;\r\n\r\ntry{\r\n\r\n\r\n// Create dropdown list\r\nList<String> dropdownList = new ArrayList<String>();\r\nString[] values = {\"One\",\"Two\",\"Three\"};\r\nfor (String value : values) {\r\n    dropdownList.add(value.trim());\r\n}\r\n\r\n// Create plugin web support\r\nPluginWebSupport pluginWebSupport = (PluginWebSupport) AppUtil.getApplicationContext().getBean(\"pluginWebSupport\");\r\n\r\n// Create dropdown menu\r\nPluginProperty dropdownMenu = new PluginProperty(\"dropdownMenu\", \"Dropdown Menu\", PluginProperty.TYPE_COMBOBOX, dropdownList, defaultValue, true, \"Select an option from the dropdown menu\");\r\n\r\n// Add dropdown menu to plugin web support\r\npluginWebSupport.addPluginProperty(AppPluginUtil.getCurrentPlugin(), dropdownMenu);\r\n}catch(Exception e){\r\n    \r\n}"
