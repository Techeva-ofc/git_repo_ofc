            "primaryKey": "id",
            "sql": "select * from ((select b.dateCreated,b.c_inspectionreportStatus,a.c_companyLocation,b.id,b.c_serReqServCat,b.c_inspSchedlDate,\nb.c_serReqDmccSrNo,b.c_status,d.c_ofc_category_name,a.c_companyName,b.c_paymentDate,c.c_tower_seq_no,c.c_tower_name \nfrom   jwdb.app_fd_ofc_serReqs_tab b join jwdb.app_fd_ofc_company_tab a   on a.id = b.c_companyName join \njwdb.app_fd_ofc_tower_tab c on a.c_tower_name = c.id join jwdb.app_fd_ofc_ofc_category_tab d on b.c_ofc_category_name = d.id \nwhere b.c_status='Payment Completed' )union all ( ( select b.dateCreated,b.c_inspectionreportStatus,a.c_companyLocation,b.id,b.c_serReqServCat,b.c_inspSchedlDate,\nb.c_serReqDmccSrNo,b.c_status,d.c_ofc_category_name,a.c_companyName,b.c_paymentDate,c.c_tower_seq_no,c.c_tower_name     \nfrom   jwdb.app_fd_ofc_serReqs_tab b join jwdb.app_fd_ofc_company_tab a on a.id = b.c_companyName join \njwdb.app_fd_ofc_tower_tab c on a.c_tower_name = c.id join jwdb.app_fd_ofc_ofc_category_tab d on b.c_ofc_category_name = d.id \nwhere b.c_status='Inspection Scheduled'))) dummy order by c_status desc, c_paymentDate asc ,  CAST(c_tower_seq_no as SIGNED INTEGER) asc"
        }
    },
