            "primaryKey": "id",
            "sql": "SELECT o.c_ofc_category_name,sr.id,sr.c_jobid,sr.c_reportdate,sr.c_licenseactivity, sr.dateCreated, sr.c_serReqDmccSrNo, c.c_licenseNumber, sr.c_serReqServCat, \r\nc.c_companyName, c.c_companyLocation, t.c_tower_seq_no, t.c_tower_name ,sr.c_status, sr.c_inspSchedlDate, sr.c_inspectionreportStatus\r\nFROM app_fd_ofc_serreqs_tab sr\r\nINNER JOIN app_fd_ofc_company_tab c\r\nON sr.c_companyName = c.id\r\nINNER JOIN app_fd_ofc_location_tab l\r\nON c.c_companyLocation = l.c_locName\r\nINNER JOIN app_fd_ofc_tower_tab t\r\nON c.c_tower_name = t.c_tower_name\r\nINNER JOIN (SELECT * FROM app_fd_ofc_ofc_category_tab WHERE c_ofc_category_name <> 'Activity Amendment') o\r\nON o.id = sr.c_ofc_category_name\r\nWHERE sr.c_status = 'Payment Completed' \r\nor sr.c_status = 'Inspection To-Be Re-Scheduled' \r\nor sr.c_status = 'Inspection Completed'\r\nor sr.c_status = 'Inspection Scheduled'\r\nor sr.c_status = 'Image(s) To-Be Uploaded'\r\nor sr.c_status = 'Images Uploaded'\r\nor sr.c_status = 'Paid Fee'\r\nor sr.c_status = 'Compliance'\r\nor sr.c_status = 'Drawing / Document Uploaded'\r\nor sr.c_status = 'Payment Completed for OFC Amendment'"
        }
    },
