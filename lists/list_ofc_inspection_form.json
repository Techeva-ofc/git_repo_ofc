            "primaryKey": "id",
            "sql": "select \r\n  b.id, \r\n  b.c_serReqDmccSrNo, \r\n  b.c_serReqUserName,\r\n  b.c_status, \r\n  b.c_paymentDate,\r\n  b.c_applicationMode,\r\n  a.c_companyName,\r\n  c.c_tower_seq_no, \r\n  c.c_tower_name,\r\n  b.c_inspectionreportStatus,\r\n  b.c_inspSchedlDate,\r\n  b.c_inspectorNames,\r\n  b.c_waiversend,\r\n  b.c_reportdate,b.c_waiverstate\r\nfrom \r\n  jwdb.app_fd_ofc_company_tab a \r\n  join jwdb.app_fd_ofc_serReqs_tab b on a.id = b.c_companyName \r\n  join jwdb.app_fd_ofc_tower_tab c on a.c_tower_name = c.id \r\n  join jwdb.app_fd_ofc_ofc_category_tab d on b.c_ofc_category_name = d.id \r\n  \r\nWHERE  \r\nb.c_inspectionreportStatus = 'Full Compliance' OR\r\nb.c_inspectionreportStatus = 'Partial Compliance' OR \r\nb.c_inspectionreportStatus =  'Fitout Non-Compliance'OR\r\nb.c_inspectionreportStatus  =  'Other Non-Compliance' OR\r\nb.c_status = 'Inspection Scheduled'\r\n\r\norder by b.c_inspSchedlDate\r\n"
        }
    },
