            "primaryKey": "id",
            "sql": "SELECT sr.c_feetopay,sr.id,\r\nsr.c_jobid,\r\nsr.c_waiverstate,\r\nsr.c_licenseactivity,\r\nsr.dateCreated,\r\nsr.c_reportdate,\r\nsr.C_serReqDmccSrNo, \r\nc.c_licenseNumber, \r\nsr.c_serReqServCat, \r\nc.c_companyName, \r\nc.c_companyLocation, \r\nt.c_tower_seq_no, \r\nt.c_tower_name ,\r\nsr.c_status, \r\nsr.c_inspSchedlDate, \r\nsr.c_inspectionreportStatus,\r\nsr.c_inspectorNames,\r\nd.c_ofc_category_name\r\nFROM \r\napp_fd_ofc_serreqs_tab sr\r\nINNER JOIN \r\n    app_fd_ofc_company_tab c\r\n    ON sr.c_companyName = c.id\r\nINNER JOIN \r\n    app_fd_ofc_location_tab l\r\n    ON c.c_companyLocation = l.c_locName\r\nINNER JOIN \r\n    app_fd_ofc_tower_tab t\r\n    ON c.c_tower_name = t.c_tower_name\r\ninner join app_fd_ofc_ofc_category_tab d \r\non sr.c_ofc_category_name = d.id \r\nWHERE  \r\nsr.c_inspectionreportStatus IN ('Full Compliance','Partial Compliance','Fitout Non-Compliance','Other Non-Compliance')\r\n\r\norder by sr.c_inspSchedlDate\r\n\r\n"
        }
    },
