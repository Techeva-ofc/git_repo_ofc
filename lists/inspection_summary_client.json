            "primaryKey": "id",
            "sql": "select b.c_image_by_client,b.c_drawingupload,b.c_jobid,b.c_reportdate,b.c_inspectionreportStatus,a.c_companyLocation,b.id,b.c_serReqServCat,b.c_inspSchedlDate,b.c_serReqDmccSrNo,b.c_status,d.c_ofc_category_name,a.c_companyName,b.c_paymentDate,c.c_tower_seq_no,c.c_tower_name \nfrom \n    jwdb.app_fd_ofc_company_tab a \njoin \n    jwdb.app_fd_ofc_serReqs_tab b on a.id = b.c_companyName \njoin\n    jwdb.app_fd_ofc_tower_tab c on a.c_tower_name = c.id \njoin \n    jwdb.app_fd_ofc_ofc_category_tab d on b.c_ofc_category_name = d.id where b.c_status not in ('Payment Pending','Inspection Scheduled','Inspection Completed') \nand b.c_inspectionreportStatus is not null \nor b.c_inspectionreportStatus = 'Full Compliance'\nor b.c_status = 'Image(s) To-Be Uploaded'\nor b.c_status = 'Awaiting payment from the client'\nor b.c_status = 'Images Uploaded'\nor b.c_drawingupload = 'drawinguploaded'\nor b.c_status = 'Pae'\n\norder by b.c_inspSchedlDate is null,b.c_inspSchedlDate desc"
        }
