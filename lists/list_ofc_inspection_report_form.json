            "optimisePaging": "",
            "primaryKey": "id",
            "sql": "SELECT \r\n  sr.id,\r\n  sr.c_image_by_client,\r\n  sr.c_jobid, \r\n  sr.c_licenseactivity, \r\n  sr.dateCreated, \r\n  sr.c_reportdate, \r\n  sr.C_serReqDmccSrNo, \r\n  c.c_licenseNumber, \r\n  sr.c_serReqServCat, \r\n  c.c_companyName, \r\n  c.c_companyLocation, \r\n  t.c_tower_seq_no, \r\n  t.c_tower_name, \r\n  sr.c_status, \r\n  sr.c_inspSchedlDate, \r\n  sr.c_inspectionreportStatus \r\nFROM \r\n  app_fd_ofc_serreqs_tab sr \r\n  join jwdb.app_fd_ofc_company_tab c ON sr.c_companyName = c.id \r\n  JOIN jwdb.app_fd_ofc_location_tab l ON c.c_companyLocation = l.c_locName \r\n  JOIN jwdb.app_fd_ofc_tower_tab t ON c.c_tower_name = t.c_tower_name \r\nWHERE \r\n  sr.c_status = 'Inspection Completed' \r\n  OR sr.c_status = 'Inspection To-Be Re-Scheduled' \r\n  OR sr.c_status = 'Inspection Scheduled' \r\n  OR sr.c_status = 'Image(s) To-Be Uploaded' \r\n  OR sr.c_status = 'Awaiting payment from the client' \r\n  OR sr.c_status = 'Images Uploaded' \r\nORDER BY \r\n  sr.c_inspSchedlDate\r\n"
        }
    },
