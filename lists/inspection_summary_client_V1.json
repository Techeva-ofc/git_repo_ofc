            "primaryKey": "id",
            "sql": "SELECT sr.c_feetopay,sr.c_waiverstate,sr.id,sr.c_reportdate,sr.c_licenseactivity, sr.dateCreated, sr.C_serReqDmccSrNo, c.c_licenseNumber, sr.c_serReqServCat, \r\nc.c_companyName, c.c_companyLocation, t.c_tower_seq_no, t.c_tower_name ,sr.c_status, sr.c_inspSchedlDate, sr.c_inspectionreportStatus,sr.c_draw_status\r\nFROM app_fd_ofc_serreqs_tab sr\r\nINNER JOIN app_fd_ofc_company_tab c\r\nON sr.c_companyName = c.id\r\nINNER JOIN app_fd_ofc_location_tab l\r\nON c.c_companyLocation = l.c_locName\r\nINNER JOIN app_fd_ofc_tower_tab t\r\nON c.c_tower_name = t.id\r\nWHERE sr.c_status = 'Closed'\r\nOR sr.c_status = 'Inspection Scheduled'\r\nOR sr.c_status = 'Awaiting payment for re-schedule'\r\nOR sr.c_status = 'Image(s) To-Be Uploaded'\r\nOR sr.c_status = 'Images Uploaded'\r\nOR sr.c_status = 'Paid Fee'\r\nOR sr.c_status = 'Compliance'\r\nOR sr.c_status = 'Drawing / Document Uploaded'\r\nOR sr.c_status = 'Payment Completed for OFC Amendment'\r\nOR sr.c_status = 'Inspection To-Be Re-Scheduled' \r\nOR sr.c_status = 'Drawing Compliance'\r\nand sr.c_serReqUserName = '#currentUser.email#'\r\norder by sr.c_inspSchedlDate"
        }
    },
